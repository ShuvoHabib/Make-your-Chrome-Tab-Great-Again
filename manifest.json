{
  "manifest_version": 2,
  "name": "Make your tab great again",
  "version": "1.0.2",
  "description": "Make your tab great again by Shuvo Habib",

  "browser_action": {
    "default_icon": "logo.png"
  },

    "chrome_url_overrides": {
      "newtab": "index.html"
    },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}